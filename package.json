{
  "name": "@newhighsco/create",
  "description": "New High Score shareable dotfiles",
  "version": "1.1.0",
  "author": "New High Score <hello@newhighsco.re>",
  "license": "ISC",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/newhighsco/create.git"
  },
  "homepage": "https://github.com/newhighsco/create#readme",
  "bugs": {
    "url": "https://github.com/newhighsco/create/issues"
  },
  "scripts": {
    "test": "npm run lint",
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint --cache --ext .js,.jsx,.json .",
    "format": "npm-run-all --parallel format:*",
    "format:js": "npm run lint:js -- --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "12.1.4",
    "@newhighsco/commitlint-config": "1.0.13",
    "@newhighsco/editor-config": "1.1.2",
    "@newhighsco/eslint-config": "2.3.0",
    "@newhighsco/prettier-config": "2.0.11",
    "@newhighsco/release-config": "1.0.63",
    "eslint": "7.28.0",
    "husky": "6.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.1",
    "semantic-release": "17.4.4"
  },
  "commitlint": {
    "extends": [
      "@newhighsco"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@newhighsco"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "prettier": "@newhighsco/prettier-config",
  "release": {
    "extends": "@newhighsco/release-config",
    "branches": [
      "main"
    ]
  },
  "renovate": {
    "extends": [
      "@newhighsco"
    ],
    "baseBranches": [
      "main"
    ]
  }
}
